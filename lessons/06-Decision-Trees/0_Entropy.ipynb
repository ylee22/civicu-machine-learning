    "def value_entropy(values):\n",
    "    values = np.array(values)\n",
    "    unique_values, counts = np.unique(values, return_counts=True)\n",
    "    freqs = counts.astype('float') / len(values)\n",
    "    return entropy(freqs)\n"
   ]
